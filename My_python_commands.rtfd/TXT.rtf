\f0\fs28 \cf25 # Draw PCA and MDS plot \
\
library(ggplot2)\
data.matrix <- as.matrix(cluster.level2)\
head(data.matrix)\
dim(data.matrix)\
pca <- prcomp(t(data.matrix), scale=TRUE, center=TRUE)\
pca.var <- pca$sdev^2\
pca.var.per <- round(pca.var/sum(pca.var)*100, 1)\
pca.var.per\
pca.data <- data.frame(Sample=rownames(pca$x),\
                       X=pca$x[,1],\
                       Y=pca$x[,2])\
pca.data\
ggplot(data=pca.data, aes(x=X, y=Y, label=Sample)) +\
  geom_text() +\
  xlab(paste("PC1 - ", pca.var.per[1], "%", sep="")) +\
  ylab(paste("PC2 - ", pca.var.per[2], "%", sep="")) +\
  theme_bw() +\
  ggtitle("PCA Graph")\
\
#Now draw an MDS plot \
distance.matrix <- dist(scale(t(data.matrix), center=TRUE, scale=TRUE),\
                        method="euclidean")\
mds.stuff <- cmdscale(distance.matrix, eig=TRUE, x.ret=TRUE)\
mds.var.per <- round(mds.stuff$eig/sum(mds.stuff$eig)*100, 1)\
mds.var.per\
mds.values <- mds.stuff$points\
mds.data <- data.frame(Sample=rownames(mds.values),\
                       X=mds.values[,1],\
                       Y=mds.values[,2])\
mds.data\
ggplot(data=mds.data, aes(x=X, y=Y, label=Sample)) +\
  geom_text() +\
  theme_bw() +\
  xlab(paste("MDS1 - ", mds.var.per[1], "%", sep="")) +\
  ylab(paste("MDS2 - ", mds.var.per[2], "%", sep="")) +\
  ggtitle("MDS plot using Euclidean distance")\
